(letrec ((fib (lambda (n)
                (if (< n 2)
                    n
                    (+ (fib (- n 1))
                       (fib (- n 2)))))))
  (spawn (lambda ()
           (display (fib 20))
           (newline)))
  (spawn (lambda ()
           (display (fib 25))
           (newline)))
  (spawn (lambda ()
           (display (fib 30))
           (newline)))
  (display (fib 35))
  (newline))
