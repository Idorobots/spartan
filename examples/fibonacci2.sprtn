(letrec ((fib (lambda (n)
                (if (< n 2)
                    n
                    (+ (fib (- n 1))
                       (fib (- n 2)))))))
  (spawn (lambda ()
           (monitor 250)))
  (spawn (lambda ()
           (display (fib 10))
           (newline)))
  (spawn (lambda ()
           (display (fib 15))
           (newline)))
  (spawn (lambda ()
           (display (fib 20))
           (newline)))
  (display (fib 25))
  (newline))
